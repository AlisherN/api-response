{
    "name": "obiefy/api-response",
    "description": "PHP package for standard formats for API responses",

    "license": "MIT",
    "authors": [
        {
            "name": "Obiefy",
            "email": "obayhamed@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Obiefy\\API\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Obiefy\\API\\Tests\\": "tests/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "stable",
    "require": {
        "php" : "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    },
    "extra": {
        "laravel": {
            "providers": [
                    "Obiefy\\API\\APIServiceProvide"
            ]
        }
    }
}
